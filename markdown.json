{
	"h1": {
		"prefix": "/h1",
		"body": "# ${1:見出し1}",
		"description": "大見出し"
	},
	"h2": {
		"prefix": "/h2",
		"body": "## ${1:見出し2}",
		"description": "中見出し"
	},
	"h3": {
		"prefix": "/h3",
		"body": "### ${1:見出し3}",
		"description": "小見出し"
	},
	"bullet": {
		"prefix": "/bullet",
		"body": "- $1",
		"description": "箇条書きリスト"
	},
	"numlist": {
		"prefix": "/numlist",
		"body": "1. $1",
		"description": "番号付きリスト"
	},
	"todo": {
		"prefix": "/todo",
		"body": "- [ ] $1",
		"description": "ToDoリスト"
	},
	"checkedtodo": {
		"prefix": "/todocheck",
		"body": "- [x] ${1:完了ToDoリスト}",
		"description": "チェック済みToDoリスト"
	},
	"div": {
		"prefix": "/div",
		"body": "---",
		"description": "区切り線"
	},
	"quote": {
		"prefix": "/quote",
		"body": "> ${1:引用文}",
		"description": "引用ブロック"
	},
	"code": {
		"prefix": "/code",
		"body": "```${1:language}\n$2\n```",
		"description": "コードブロック"
	},
	"inline_code": {
		"prefix": "/code_inline",
		"body": "`$1`",
		"description": "インラインコード"
	},
	"text": {
		"prefix": "/text",
		"body": "$1",
		"description": "新しいテキストブロック"
	},
	"today": {
		"prefix": "/today",
		"body": "`${CURRENT_YEAR}-${CURRENT_MONTH}-${CURRENT_DATE}`",
		"description": "日付スタンプ"
	},
	"table": {
		"prefix": "/table",
		"body": [
			"| ${1:ヘッダー1} | ${2:ヘッダー2} | ${3:ヘッダー3} |",
			"| ------------- | ------------- | ------------- |",
			"| ${4:データ1}  | ${5:データ2}  | ${6:データ3}  |"
		],
		"description": "テーブルテンプレート"
	},
	"math": {
		"prefix": "/math",
		"body": "$$ ${1:数式} $$",
		"description": "LaTeX形式の数学式"
	},
	"comment": {
		"prefix": "/comment",
		"body": "<!-- ${1:コメント文} -->",
		"description": "コメント"
	},
	"delete": {
		"prefix": "/delete",
		"body": "<!-- 削除予定: ${1:内容} -->",
		"description": "削除ブロック（コメント化）"
	},
	"pien": {
		"prefix": "/pien",
		"body": "🥺",
		"description": "絵文字ピッカー（ピエン）"
	},
	"link": {
		"prefix": "/link",
		"body": "[${1:リンクテキスト}](${2:https://example.com})",
		"description": "文字だけのリンク"
	},
	"image_link": {
		"prefix": "/imagelink",
		"body": "[![${1:alt}](${2:https://example.com/thumbnail.jpg}${4})](${3:https://example.com}${5})",
		"description": "画像＋リンク"
	},
	"embed": {
		"prefix": "/embed",
		"body": "[埋め込みコンテンツ](${1:https://example.com})",
		"description": "埋め込みリンク"
	},
	"toc": {
		"prefix": "/toc",
		"body": "<!-- TOC -->\n<!-- /TOC -->",
		"description": "目次ブロック"
	}
	// Place your snippets for markdown here. Each snippet is defined under a snippet name and has a prefix, body and 
	// description. The prefix is what is used to trigger the snippet and the body will be expanded and inserted. Possible variables are:
	// $1, $2 for tab stops, $0 for the final cursor position, and ${1:label}, ${2:another} for placeholders. Placeholders with the 
	// same ids are connected.
	// Example:
	// "Print to console": {
	// 	"prefix": "log",
	// 	"body": [
	// 		"console.log('$1');",
	// 		"$2"
	// 	],
	// 	"description": "Log output to console"
	// }
}